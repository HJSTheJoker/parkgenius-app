# Environment Variables Example
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database URLs (Optional - for additional databases)
PLANETSCALE_DATABASE_URL=your_planetscale_url
FAUNA_SECRET_KEY=your_fauna_secret
REDIS_URL=your_redis_url

# External API Keys
QUEUE_TIMES_API_KEY=your_queue_times_api_key
WEATHER_API_KEY=your_weather_api_key

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=ParkGenius

# Authentication (Optional)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
NEXT_PUBLIC_GITHUB_CLIENT_ID=your_github_client_id

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=your_google_analytics_id
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key

# Encryption Key (Generate a secure random string)
ENCRYPTION_KEY=your_encryption_key

# Machine Learning API (Optional)
ML_API_ENDPOINT=your_ml_api_endpoint
ML_API_KEY=your_ml_api_key